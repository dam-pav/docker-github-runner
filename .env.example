# Copy this file to .env and fill in the values before running `docker compose up -d`

RUNNER_URL=https://github.com/dam-pav/github-runner-docker

# Registration version and token (generate from GitHub > Settings > Actions > Runners)
RUNNER_VERSION=2.331.0
RUNNER_TOKEN=

# Optional
RUNNER_NAME=github-runner
RUNNER_WORKDIR=_work
RUNNER_LABELS= self-hosted,x64,linux
